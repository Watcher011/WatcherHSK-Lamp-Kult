	<ThingDef ParentName="SK_SculptureBase">
		<defName>GreenyArtFl</defName>
		<label>скульптура Грини с факелом</label>
		<uiIconPath>Things/Building/Art/GreenyFl_MenuIcon</uiIconPath>
		<description>Скульптура Грини дарующего свет для колонии. Ходят слухи что по ночам статуи сбегают.</description>
		<graphicData>
			<graphicClass>Graphic_Multi</graphicClass>
			<shadowData>
				<volume>(0, 0, 0)</volume>
			</shadowData>
			<shaderType>CutoutComplex</shaderType>
			<texPath>Things/Building/Art/GreenyFl</texPath>
			<drawSize>(3,3)</drawSize>
		</graphicData>
		<fillPercent>0.5</fillPercent>
		<rotatable>false</rotatable>
		<tickerType>Normal</tickerType>
		<drawerType>RealtimeOnly</drawerType>
		<minifiedDef>MinifiedThing</minifiedDef>
		<statBases>
			<MaxHitPoints>170</MaxHitPoints>
			<Beauty>70</Beauty>
			<WorkToMake>36000</WorkToMake>
			<Bulk>80</Bulk>
			<Mass>50</Mass>
			<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">48</StyleDominance>
		</statBases>
		<placeWorkers>
			<li>PlaceWorker_Heater</li>
			<li>PlaceWorker_MeditationOffsetBuildingsNear</li>
		</placeWorkers>
		<stuffCategories>
			<li>StrongMetallic</li>
			<li>Precious</li>
			<li>Woody</li>
			<li>Stony</li>
			<li>Ceramic</li>
		</stuffCategories>
		<costStuffCount>170</costStuffCount>
		<costList>
			<Chemfuel>20</Chemfuel>
		</costList>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>9</glowRadius>
				<glowColor>(217,177,113,0)</glowColor>
			</li>
			
			<li Class="SK.CompProperties_FireOverlayMod">
				<fireSize>0.6</fireSize>
				<offset>(1,-1,0.30)</offset>
			</li>
			<li Class="CompProperties_MeditationFocus">
				<statDef>MeditationFocusStrength</statDef>
				<focusTypes>
					<li>Flame</li>
				</focusTypes>
				<offsets>
					<li Class="FocusStrengthOffset_Lit">
						<offset>0.1</offset>
					</li>
					<li Class="FocusStrengthOffset_BuildingDefsLit">
						<defs>
							<li>Campfire</li>
							<li>BowlLamp</li>
							<li>Candle</li>
							<li>TorchLamp</li>
							<li MayRequire="Ludeon.RimWorld.Royalty">Brazier</li>
							<li MayRequire="Ludeon.RimWorld.Royalty,Ludeon.RimWorld.Ideology">DarklightBrazier</li>
							<li MayRequire="Ludeon.RimWorld.Ideology">Darktorch</li>
							<li MayRequire="Ludeon.RimWorld.Ideology">DarktorchFungus</li>
						</defs>
						<radius>9.9</radius>
						<maxBuildings>8</maxBuildings>
						<offsetPerBuilding>0.01</offsetPerBuilding>
						<explanationKey>MeditationFocusPerFlame</explanationKey>
						<explanationKeyAbstract>MeditationFocusPerFlameAbstract</explanationKeyAbstract>
					</li>
				</offsets>
			</li>
		</comps>
		<recipeMaker>
			<researchPrerequisite>Art_C1</researchPrerequisite>
		</recipeMaker>
	</ThingDef>

</Defs>